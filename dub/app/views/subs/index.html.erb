<h1>Listing subs</h1>

<table>
  <tr>
    <th>Stime</th>
    <th>Etime</th>
    <th>Source</th>
    <th>Trans</th>
    <th>Lang</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @subs.each do |sub| %>
  <tr>
    <td><%= sub.sTime %></td>
    <td><%= sub.eTime %></td>
    <td><%= sub.source %></td>
    <td><%= sub.trans %></td>
    <td><%= sub.lang %></td>
    <td><%= link_to 'Show', sub %></td>
    <td><%= link_to 'Edit', edit_sub_path(sub) %></td>
    <td><%= link_to 'Destroy', sub, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sub', new_sub_path %>
